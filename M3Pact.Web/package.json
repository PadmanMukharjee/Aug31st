    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "npm run clean:dist-dev && ng build  --output-path=dist-dev --env=development --verbose",
    "clean:dist-dev": "npm run rimraf -- dist-dev",
    "build:qa": "npm run clean:dist-qa && ng build  --output-path=dist-qa --env=qa --verbose",
    "clean:dist-qa": "npm run rimraf -- dist-qa",
    "build:prod": "npm run clean:dist-qa && ng build  --output-path=dist-prod --aot --output-hashing all --build-optimizer true --env=prod --verbose",
    "clean:dist-prod": "npm run rimraf -- dist-prod",
    "rimraf": "rimraf"
  },
